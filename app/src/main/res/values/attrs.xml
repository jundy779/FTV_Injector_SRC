        <!--<attr name="mwWaveHeight" format="dimension"/>-->
        <!--<attr name="mwScaleX" format="float"/>-->
        <!--<attr name="mwScaleY" format="float"/>-->
    <!--</declare-styleable>-->
    
        <!-- Location of the SVG document. -->
        
    
      
     <declare-styleable name="SVGImageView">
     
        <!-- Location of the SVG document. -->
        <attr name="svg" format="reference|string"/>
        <attr name="css" format="string"/>
    </declare-styleable>
    <attr name="updatesContinuously" format="boolean" />
    <attr name="preferenceCategoryStyle" format="reference" />
     
    <attr name="preferenceCategoryTitleTextAppearance" format="reference" />
    <attr name="preferenceCategoryTitleTextColor" format="reference|color" />
</resources>




